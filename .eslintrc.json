{
  "env": { "browser": true, "es2021": true },
  "extends": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint/eslint-plugin"],
  "ignorePatterns": ["**/*.d.ts"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "error"
  }
}
